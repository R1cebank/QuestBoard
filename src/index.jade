doctype html
html
  head
    title QuestBoard
    meta(charset = 'utf-8')
    meta(name='viewport', content='width=device-width,initial-scale=1,maximum-scale=1,user-scalable=no')
    
    each url in stylesheets
      link(rel = 'stylesheet', href = url)  
          
  body(ng-app = 'Questboard.app')
    
    h1 Hello Questboard!
    
    div(ui-view)
    
    each url in scripts
      script(src = url)
